<%@ page contentType="text/html; charset=utf-8" %>
<html>
<head>
<title>Scripting tag</title>
</head>
<body>
	<%
		// 스크립플릿 태그에 반복문 사용 0 부터 10까지 짝수만 출력 -> i%2 == 0 
		for(int i=0; i<=10; i++){
			if(i%2==0)
				out.println(i+"<br>");
		}
	%>
	<%
	// 스크립플릿 태그에 반복문 사용 0 부터 10까지 3의 배수만 출력 -> i%3 == 0
		for(int i=0; i<=20; i++){
			if(i%3==0)
				out.println(i+"<br>");
		}
	%>
</body>
</html>